<div class="card"> 
  <div class="card_header">
    <h5>Sign Up</h5> 
  </div> 
  <div class="card_body">
   <form id="signup-form">
      <label for="username-signup" class="form_label">Username:</label>
      <input type="text" id="username-signup" class="form_input" autocomplete="off"/>

      <label for="email-signup" class="form_label">Email:</label>
      <input type="text" id="email-signup" class="form_input" autocomplete="off"/>

      <label for="password-signup" class="form_label">Password:</label>
      <input type="password" id="password-signup" class="form_input" autocomplete="off"/>

      <button type="submit" class="btn">Sign Up!</button> 
   </form>       
  </div>
</div>
<script src="/javascript/signup.js"></script>